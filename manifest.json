{
    "name": "New Tab Notepad",
    "version": "1.01",
    "description": "A simplistic notepad that opens up every time you open your web browser.",
    "permissions": ["storage"],
    "manifest_version": 2,
    "chrome_url_overrides" : {
        "newtab": "index.html"
    },
    "icons": { "16": "img/icon16.png",
      "48": "img/icon48.png",
     "128": "img/icon128.png" }
}